{
    "_comment": "http://www.speich.net/articles/2013/07/19/",
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "scripts/js/geodesy/latlon-spherical.js",
            "scripts/js/geodesy/latlon-ellipsoidal.js",
            "scripts/js/geodesy/latlon-vincenty.js",
            "scripts/js/geodesy/latlon-vectors.js",
            "scripts/js/geodesy/vector3d.js",
            "scripts/js/geodesy/osgridref.js",
            "scripts/js/geodesy/utm.js",
            "scripts/js/geodesy/mgrs.js",
            "scripts/js/geodesy/dms.js",
            "scripts/js/geodesy/README.md"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "exclude": [ "docs", "node_modules" ]
    },
    "plugins": [ "plugins/markdown" ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "systemName": "remoteFileExplorer",
        "footer": "Created by Simon Speich",
        "copyright": "2013",
        "navType": "vertical",
        "theme": "spacelab",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": false
    },
    "opts": {
        "template": "/opt/jsdoc/templates/default/",
        "encoding": "utf8",
        "destination": "scripts/js/geodesy/docs",
        "recurse": true,
        "private": true
    }
}
